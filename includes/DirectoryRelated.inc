<?phpfunction GenerateFullImageDirectoryPath($idnum){	for ($rep = 1; (empty($SubDir));	$rep++)		{	$upperbound = 16777200 - ( 400 * ( $rep - 1 ) );			$lowerbound = 16777200 - ( 400 * $rep );			if ( ( $idnum <= $upperbound ) && ( $idnum > $lowerbound ) )				{		$SubDir = $lowerbound . "_" . $upperbound;			}		}	for ($rep = 1; (empty($MastrDir));	$rep++)		{	$upperbound = 16777200 - ( 160000 * ( $rep - 1 ) );			$lowerbound = 16777200 - ( 160000 * $rep );			if ( ( $idnum <= $upperbound ) && ( $idnum > $lowerbound ) )				{		$MastrDir = $lowerbound . "_" . $upperbound;		}		}return ($MastrDir . '/' . $SubDir . '/' . $idnum . '/');	}function MakeUploadDirectoryLinks($idnum,$SubDir){	$HexLowerBound = 65536;	$HexUpperBound = 1048575;	$SymLinkHex = dechex(rand($HexLowerBound,$HexUpperBound)) . dechex($idnum);	$ImgTmpLnk = "../tmp/img/" . $SymLinkHex;	$AudTmpLnk = "../tmp/aud/" . $SymLinkHex;//	$PrfTmpLnk = "../tmp/img/" . $SymLinkHex . ".prf";//	$ListsTmpLnk = "../tmp/lists/" . $SymLinkHex;		$sh_MkUploadLnks =	"ln -s /cluster/harry/img/photo/" . $SubDir . " " . $ImgTmpLnk				."\n".	"ln -s /cluster/harry/aud/mp3/" . $SubDir . " " . $AudTmpLnk//				."\n".	"ln -s /cluster/harry/img/photo/" . " " . $PrfTmpLnk//				."\n".	"ln -s /cluster/harry/lists/aud/" . $SubDir . " " . $ListsTmpLnk				;					exec($sh_MkUploadLnks);		$output = $SymLinkHex . "/";	return $output;		}function MakeCurrLocLinks($idnum,$SubDir,$OwnerLink){	$TmpLink = substr($OwnerLink,0,11) . ".tmp";	$ImgTmpLnk = "../tmp/img/{$TmpLink}";	$AudTmpLnk = "../tmp/aud/{$TmpLink}";//	$ListsTmpLnk = "../tmp/lists/{$TmpLink}";		$sh_MkUploadLnks =	"rm {$ImgTmpLnk}"				."\n".	"rm {$AudTmpLnk}"//				."\n".	"rm {$ListsTmpLnk}"				."\n".	"ln -s /cluster/harry/img/photo/" . $SubDir . " " . $ImgTmpLnk				."\n".	"ln -s /cluster/harry/aud/mp3/" . $SubDir . " " . $AudTmpLnk//				."\n".	"ln -s /cluster/harry/lists/aud/" . $SubDir . " " . $ListsTmpLnk				;					exec($sh_MkUploadLnks);		$output = $TmpLink;	return $output;		}?>