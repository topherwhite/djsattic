<?phpfunction GenerateTableGroup($id){	$grpSize = 400;	for ($rep = 1; (empty($Tble)); $rep++)	{	$upbnd = 16777200-($grpSize*($rep-1));		$lwbnd = 16777200-($grpSize*$rep);		if (($id <= $upbnd) && ($id > $lwbnd))		{	$grp = dechex($lwbnd) ."_". dechex($upbnd);			$Tble['lists'] = "lists.lists_".$grp;			$Tble['uploads'] = "uploads.uploads_".$grp;			$Tble['frnds'] = "frnds.frnds_".$grp;			$Tble['visits'] = "visits.visits_".$grp;			$Tble['lgn'] = "lgn.lgn_".$grp;						$Tble['msgs_rec'] = "msgs.msgs_rec_".$grp;			$Tble['msgs_snt'] = "msgs.msgs_snt_".$grp;			$Tble['msgs_arc'] = "msgs.msgs_arc_".$grp;			$Tble['msgs_body'] = "msgs.msgs_body_".$grp;						$Tble['grps'] = "grps.grps_".$grp;			$Tble['grps_blng'] = "grps.blng_".$grp;			$Tble['grps_mmbr'] = "grps.mmbr_".$grp;						$Tble['albms'] = "albms.albms_".$grp;			$Tble['ad_clicks'] = "ads.clicks_".$grp;				}	}return $Tble;}?>