		" " . $AudBaseDir . $MastrDir . "/" . $SubDir . "/" . $idnum				.			" " . $AudBaseDir . $MastrDir . "/" . $SubDir . "/" . $idnum . "/drop"							."\n".	"chmod a+rw"				.			" " . $ImgBaseDir . $MastrDir				.			" " . $ImgBaseDir . $MastrDir . "/" . $SubDir				.			" " . $ImgBaseDir . $MastrDir . "/" . $SubDir . "/" . $idnum				.			" " . $ImgBaseDir . $MastrDir . "/" . $SubDir . "/" . $idnum . "/drop"				.			" " . $AudBaseDir . $MastrDir				.			" " . $AudBaseDir . $MastrDir . "/" . $SubDir				.			" " . $AudBaseDir . $MastrDir . "/" . $SubDir . "/" . $idnum				.			" " . $AudBaseDir . $MastrDir . "/" . $SubDir . "/" . $idnum . "/drop"					;										exec($sh_MakeUploadDirs);}?>